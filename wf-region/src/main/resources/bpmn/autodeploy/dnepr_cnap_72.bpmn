      </extensionElements>
    </serviceTask>
    <sequenceFlow id="flow5" name="${status == 'attr2_finish'}" sourceRef="exclusivegateway1" targetRef="mailtask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${status == 'attr2_finish'}]]></conditionExpression>
    </sequenceFlow>
    <serviceTask id="mailtask3" name="Відправка e-mail об отправки в Управління інфраструктури Дніпропетровської облдержадміністрації" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to">
          <activiti:expression>${email}</activiti:expression>
        </activiti:field>
        <activiti:field name="from">
          <activiti:string><![CDATA[noreply@igov.org.ua]]></activiti:string>
        </activiti:field>
        <activiti:field name="subject">
